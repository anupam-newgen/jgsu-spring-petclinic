pipeline {
	agent any
	
	stages {
		
		stage("checkout") {
			steps{
				echo "checking out the code"
			}
			
		}
		
		stage("build") {
			steps{
				echo "building out the code"
			}
		}
		
		stage("test") {
			steps{
				echo "testing out the code"
			}
		}
		
		stage ("deploy") {
			steps{
				echo "deploying out the code"
			}
		}
	}
}